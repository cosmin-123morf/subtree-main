{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.2.0",
    "generators": {
      "pets": {
        "generatorName": "typescript-axios",
        "output": "generated/pets/api-reference",
        "glob": "domains/pets/contract.yaml",
        "additionalProperties": {
          "withSeparateModelsAndApi": true,
          "enumNameSuffix": "",
          "modelPackage": "models",
          "apiPackage": "apis"
        }
      },
      "weather": {
        "generatorName": "typescript-axios",
        "output": "generated/weather/api-reference",
        "glob": "domains/weather/contract.yaml",
        "additionalProperties": {
          "withSeparateModelsAndApi": true,
          "enumNameSuffix": "",
          "modelPackage": "models",
          "apiPackage": "apis"
        }
      }
    }
  }
}
